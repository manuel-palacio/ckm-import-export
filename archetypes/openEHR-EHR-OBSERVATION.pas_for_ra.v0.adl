													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0052,    -- No
															at0053]    -- Yes
														}
													}
												}
											}
											ELEMENT[at0008] occurrences matches {0..1} matches {    -- Dress and groom score
