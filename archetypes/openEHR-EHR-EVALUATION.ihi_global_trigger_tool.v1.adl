									0  	-- assumed value
								}
							}
							ELEMENT[at0166] occurrences matches {0..1} matches {	-- 51. P8 - General anesthesia
								value matches {
									0|[local::at0167], 	-- Not applicable
									1|[local::at0168], 	-- No
									100|[local::at0237]; 	-- Yes
									0  	-- assumed value
								}
							}
							ELEMENT[at0169] occurrences matches {0..1} matches {	-- 5. Total Perinatal Module Triggers
								value matches {
									DV_COUNT matches {
										magnitude matches {|0..8|}
									}
								}
							}
						}
					}
					CLUSTER[at0170] occurrences matches {0..1} matches {	-- 6. Emergency Department Module Triggers
						items cardinality matches {1..*; ordered} matches {
							ELEMENT[at0171] occurrences matches {0..1} matches {	-- 52. E1 - Readmission to ED within 48 hours
								value matches {
									0|[local::at0172], 	-- Not applicable
									1|[local::at0173], 	-- No
									100|[local::at0238]; 	-- Yes
									0  	-- assumed value
								}
							}
							ELEMENT[at0174] occurrences matches {0..1} matches {	-- 53. E2 - Time in ED greater than 6 hours
								value matches {
									0|[local::at0175], 	-- Not applicable
									1|[local::at0176], 	-- No
									100|[local::at0239]; 	-- Yes
									0  	-- assumed value
								}
							}
							ELEMENT[at0194] occurrences matches {0..1} matches {	-- 6. Total Emergency Department Module Triggers
								value matches {
									DV_COUNT matches {
										magnitude matches {|0..2|}
									}
								}
							}
						}
					}
					ELEMENT[at0177] occurrences matches {0..1} matches {	-- 7. Total Sum of Triggers
						value matches {
							DV_COUNT matches {
								magnitude matches {|0..53|}
							}
						}
					}
					ELEMENT[at0258] occurrences matches {0..1} matches {	-- 8. Additional comments (other triggers or AE identified)
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Global Trigger Tool (GTT) for measuring Adverse Events">
					description = <"The IHI Global Trigger Tool (GTT) is used to measure the prevalence of possible Adverse Events in in-patient's records. It uses \"triggers\" (or clues), that have been identified to occur commonly in cases where the patient has suffered harm or injury related to medical care. The tool must only be used if the record complies to the following pre-conditions: (1) patient >= 18 years old, (2) patient's length of stay > 24 hours, (3) patient record must be closed and codified, and (4) patient was NOT inpatient psychiatric or rehabilitation patient.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"1. Care Modules Triggers">
					description = <"*">
				>
				["at0003"] = <
					text = <"01. C1 - Transfusion of Blood or Use of Blood Products">
					description = <"Any transfusion of packed red blood cells or whole blood should be investigated for causation, including excessive bleeding (surgical or anticoagulant-related), unintentional trauma of a blood vessel, etc.">
				>
				["at0004"] = <
					text = <"No">
					description = <"*">
				>
				["at0005"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0006"] = <
					text = <"02. C2 - Code, Cardiac or Pulmonary Arrest, or Rapid Response Team Activation">
					description = <"All “codes,” cardiac or pulmonary arrests, and activation of Rapid Response Teams need to be carefully reviewed as this may be the culmination of an adverse event (check for medication-related issues).">
				>
				["at0007"] = <
					text = <"No">
					description = <"*">
				>
				["at0008"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0009"] = <
					text = <"03. C3 - Acute Dialysis">
					description = <"A new need for dialysis may be the course of a disease process or the result of an adverse event. Examples of adverse events might be drug-induced renal failure or reaction to the administration of a dye for radiological procedures.">
				>
				["at0010"] = <
					text = <"No">
					description = <"*">
				>
				["at0011"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0012"] = <
					text = <"04. C4 - Positive Blood Culture">
					description = <"A positive blood culture at any time during the hospitalization must be investigated as an indicator of an adverse event, specifically a hospital-associated infection.">
				>
				["at0013"] = <
					text = <"No">
					description = <"*">
				>
				["at0014"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0015"] = <
					text = <"05. C5 - X-Ray or Doppler Studies for Emboli or Deep Vein Thrombosis">
					description = <"Development of a deep vein thrombosis (DVT) or pulmonary embolism (PE) during a hospital stay in most cases will be an adverse event. Rare exceptions may be those related to disease processes such as cancer or clotting disorders. ">
				>
				["at0016"] = <
					text = <"No">
					description = <"*">
				>
				["at0017"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0018"] = <
					text = <"06. C6 - Decrease in Hemoglobin or Hematocrit of 25% or Greater">
					description = <"Any decrease of 25 percent or greater in hemoglobin (Hg) grams or hematocrit (Hct) should be investigated, especially when occurring in a relatively short period of time such as 72 hours or less. Bleeding events are commonly identified by this trigger and may be related to use of anticoagulants or aspirin or a surgical misadventure.">
				>
				["at0019"] = <
					text = <"No">
					description = <"*">
				>
				["at0020"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0022"] = <
					text = <"07. C7 - Patient Fall">
					description = <"A fall in a care setting represents a failure of care and may be the result of medications, equipment failure, or failure of adequate staffing.">
				>
				["at0023"] = <
					text = <"No">
					description = <"*">
				>
				["at0024"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0025"] = <
					text = <"08. C8 - Pressure Ulcers">
					description = <"Pressure or decubitus ulcers are adverse events. Chronic decubiti are adverse events if they occurred during a hospitalization. If the ulcers occurred in the outpatient setting, consider the etiology (over-sedation, etc.) to assess if an adverse event occurred.">
				>
				["at0026"] = <
					text = <"No">
					description = <"*">
				>
				["at0027"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0028"] = <
					text = <"09. C9 - Readmission within 30 Days">
					description = <"Any readmission, particularly within 30 days of discharge, could be an adverse event. ">
				>
				["at0029"] = <
					text = <"No">
					description = <"*">
				>
				["at0030"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0031"] = <
					text = <"10. C10 - Restraint Use">
					description = <"Whenever restraints are used, review the documented reasons and evaluate the possible relationship between the use of restraints and confusion from drugs, etc., which would indicate an adverse event.">
				>
				["at0032"] = <
					text = <"No">
					description = <"*">
				>
				["at0033"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0034"] = <
					text = <"11. C11 - Healthcare-Associated Infections">
					description = <"Any infection occurring after admission to the hospital is likely an adverse event, especially those related to procedures or devices. Infections that cause admission to the hospital should be reviewed to determine whether they are related to medical care (e.g., prior procedure, urinary catheter at home or in long-term care) versus naturally occurring disease (e.g., ommunity-acquired pneumonia). ">
				>
				["at0035"] = <
					text = <"No">
					description = <"*">
				>
				["at0036"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0037"] = <
					text = <"12. C12 - In-Hospital Stroke">
					description = <"Evaluate the cause of the stroke to determine whether it is associated with a procedure (e.g., surgical procedure, conversion of atrial fibrillation) or anticoagulation.">
				>
				["at0038"] = <
					text = <"No">
					description = <"*">
				>
				["at0039"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0040"] = <
					text = <"13. C13 - Transfer to Higher Level of Care">
					description = <"Transfers to a higher level of care within the institution, to another institution, or to your institution from another must be reviewed. All transfers are likely to be the result of an adverse event and a patient’s clinical condition may have deteriorated secondary to an adverse event.">
				>
				["at0041"] = <
					text = <"No">
					description = <"*">
				>
				["at0042"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0043"] = <
					text = <"14. C14 - Any Procedure Complication">
					description = <"A complication resulting from any procedure is an adverse event. ">
				>
				["at0044"] = <
					text = <"No">
					description = <"*">
				>
				["at0045"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0046"] = <
					text = <"15. C15 - Other triggers or adverse events related to Care">
					description = <"Frequently when the record is reviewed, an adverse event is uncovered that does not fit a
trigger. Any such event can be placed under this “Other” trigger. An event does not require a listed trigger to be counted as an event. ">
				>
				["at0047"] = <
					text = <"No">
					description = <"*">
				>
				["at0048"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0049"] = <
					text = <"2. Medication Module Triggers">
					description = <"*">
				>
				["at0050"] = <
					text = <"1. Total Care Module Triggers">
					description = <"Total sum of Care Module Triggers">
				>
				["at0054"] = <
					text = <"16. M1 - Clostridium difficile Positive Stool">
					description = <"A positive C. difficile assay is an adverse event if a history of antibiotic use is present. ">
				>
				["at0055"] = <
					text = <"No">
					description = <"*">
				>
				["at0056"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0057"] = <
					text = <"17. M2 - Partial Thromboplastin Time (PTT) Greater than 100 Seconds">
					description = <"Elevated PTT measurements occur when patients are on heparin. Look for evidence of bleeding to determine if an adverse event has occurred.">
				>
				["at0058"] = <
					text = <"No">
					description = <"*">
				>
				["at0059"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0060"] = <
					text = <"18. M3 - International Normalized Ratio (INR) Greater than 6">
					description = <"Look for evidence of bleeding to determine if an adverse event has occurred. An elevated INR in itself is not an adverse event.">
				>
				["at0061"] = <
					text = <"No">
					description = <"*">
				>
				["at0062"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0063"] = <
					text = <"19. M4 - Glucose Less than 50 mg/dl ">
					description = <"Review for symptoms such as lethargy and shakiness documented in nursing notes, and the administration of glucose, orange juice, or other intervention.">
				>
				["at0064"] = <
					text = <"No">
					description = <"*">
				>
				["at0065"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0066"] = <
					text = <"20. M5 - Rising BUN or Serum Creatinine Two Times (2x) over Baseline ">
					description = <"Review laboratory records for rising levels of either BUN or serum creatinine. If a change of two times greater than baseline levels is found, review medication administration records for medications known to cause renal toxicity. ">
				>
				["at0067"] = <
					text = <"No">
					description = <"*">
				>
				["at0068"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0069"] = <
					text = <"21. M6 - Vitamin K Administration">
					description = <"If Vitamin K was used as a response to a prolonged INR, review the record for evidence of bleeding. An adverse event has likely occurred if there are laboratory reports.indicating a drop in hematocrit or guiac-positive stools.">
				>
				["at0070"] = <
					text = <"No">
					description = <"*">
				>
				["at0071"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0072"] = <
					text = <"22. M7 - Diphenhydramine (Benadryl) Administration">
					description = <"Diphenhydramine is frequently used for allergic reactions to drugs but can also be ordered
as a sleep aid, a pre-op/pre-procedure medication, or for seasonal allergies.">
				>
				["at0073"] = <
					text = <"No">
					description = <"*">
				>
				["at0074"] = <
					text = <"23. M8 - Romazicon (Flumazenil) Administration">
					description = <"Romazicon reverses the effect of benzodiazepine drugs. Determine why the drug was used.">
				>
				["at0075"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0076"] = <
					text = <"No">
					description = <"*">
				>
				["at0077"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0078"] = <
					text = <"24. M9 - Naloxone (Narcan) Administration">
					description = <"Naloxone is a powerful narcotic antagonist. Usage likely represents an adverse event except in cases of drug abuse or self-inflicted overdose.">
				>
				["at0079"] = <
					text = <"No">
					description = <"*">
				>
				["at0080"] = <
					text = <"25. M10 - Anti-Emetic Administration">
					description = <"Nausea and vomiting commonly are the result of drug administrations both in surgical and non-surgical settings. Anti-emetics are commonly administered.">
				>
				["at0081"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0082"] = <
					text = <"No">
					description = <"*">
				>
				["at0083"] = <
					text = <"26. M11 - Over-Sedation/Hypotension">
					description = <"Review the physician progress, nursing, or multidisciplinary notes for evidence of oversedation and lethargy.">
				>
				["at0084"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0085"] = <
					text = <"No">
					description = <"*">
				>
				["at0086"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0087"] = <
					text = <"27. M12 - Abrupt Medication Stop">
					description = <"Although the discontinuation of medications is a common finding in the record, abruptly stopping medications is a trigger requiring further investigation for cause. A sudden change in patient condition requiring adjustment of medications is often related to an adverse event.">
				>
				["at0088"] = <
					text = <"No">
					description = <"*">
				>
				["at0089"] = <
					text = <"28. M13 - Other triggers or adverse events related to Medication">
					description = <"Use this trigger for adverse drug events detected but not related to one of the Medication triggers listed above.">
				>
				["at0090"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0091"] = <
					text = <"No">
					description = <"*">
				>
				["at0092"] = <
					text = <"Yes">
					description = <"*">
				>
				["at0093"] = <
					text = <"2. Total Medication Module Triggers">
					description = <"Total sum of Medication Module Triggers">
				>
				["at0095"] = <
					text = <"3. Surgery Module Triggers">
					description = <"Total sum of Surgery Module Triggers">
				>
				["at0096"] = <
					text = <"29. S1 - Return to surgery">
					description = <"A return to the operating room can either be planned or unplanned, and both can be a result of an adverse event.">
				>
				["at0097"] = <
					text = <"Not applicable">
					description = <"*">
				>
				["at0098"] = <
					text = <"No">
					description = <"*">
				>
				["at0099"] = <
					text = <"30. S2 - Change in procedure">
					description = <"When the procedure indicated on the post-operative notes is different from the procedure planned in the pre-operative notes or documented in the surgical consent, a reviewer should look for details as to why the change occurred.">
				>
				["at0100"] = <
					text = <"Not applicable">
					description = <"*">
				>
				["at0101"] = <
					text = <"31. S3 - Admission to intensive care post-op">
					description = <"Admission to an intensive care unit can be either a normal post-operative journey or it may be unexpected. The unexpected admissions frequently are related to operative adverse events.">
				>
				["at0102"] = <
					text = <"No">
					description = <"*">
				>
				["at0103"] = <
					text = <"Not applicable">
					description = <"*">
				>
				["at0104"] = <
					text = <"32. S4 - Intubation, Reintubation, or BiPap use in Post Anesthesia Care Unit (PACU)">
					description = <"Anesthesia, sedatives, or pain medications can result in respiratory depression requiring the use of BiPap or reintubation post-operatively, which would be an adverse event.">
				>
				["at0105"] = <
					text = <"No">
					description = <"*">
				>
				["at0106"] = <
					text = <"Not applicable">
					description = <"*">
				>
				["at0107"] = <
					text = <"No">
					description = <"*">
				>
				["at0108"] = <
					text = <"33. S5 - X-ray intra-op or in PACU">
					description = <"Imaging of any kind that is not routine for the procedure requires investigation.">
				>
				["at0109"] = <
					text = <"Not applicable">
					description = <"*">
				>
				["at0110"] = <
					text = <"No">
					description = <"*">
				>
				["at0111"] = <
					text = <"34. S6 - Intra-op or post-op death">
					description = <"All deaths that occur intra-operatively should be considered adverse events unless death is clearly expected and the surgery was of a heroic nature.">
				>
				["at0112"] = <
					text = <"Not applicable">
					description = <"*">
				>
				["at0113"] = <
					text = <"No">
					description = <"*">
				>
				["at0114"] = <
					text = <"35. S7 - Mechanical ventilation greater than 24 hours post-op">
					description = <"Short-term mechanical ventilation post-operatively for cardiac, major thoracic, and certain abdominal procedures is planned. ">
				>
				["at0115"] = <
					text = <"Not applicable">
					description = <"*">
				>
				["at0116"] = <
					text = <"No">
					description = <"*">
				>
				["at0117"] = <
					text = <"36. S8 - Intra-op epinephrine, norepinephrine, naloxone, or romazicon">
					description = <"These medications are not routinely administered intra-operatively. Review anesthesia and operative notes to determine the reason for administration.">
				>
				["at0118"] = <
					text = <"Not applicable">
					description = <"*">
				>
				["at0119"] = <
					text = <"No">
					description = <"*">
				>
				["at0120"] = <
					text = <"37. S9 - Post-op troponin level greater than 1.5 ng/ml">
					description = <"A post-operative increase in troponin levels may indicate a cardiac event.">
				>
				["at0121"] = <
					text = <"Not applicable">
					description = <"*">
				>
				["at0122"] = <
					text = <"No">
					description = <"*">
				>
				["at0123"] = <
					text = <"38. S10 - Injury, repair, or removal of organ">
					description = <"Review operative notes and post-operative notes for evidence that the procedure included repair or removal of any organ. The removal or repair must be part of the planned procedure or this is an adverse event and likely the result of surgical misadventure such as an accidental injury.">
				>
				["at0124"] = <
					text = <"Not applicable">
					description = <"*">
				>
				["at0125"] = <
					text = <"No">
					description = <"*">
				>
				["at0126"] = <
					text = <"39. S11 - Any operative complication">
					description = <"This refers to any of a number of complications, including but not limited to Pulmonary Embolism (PE), Deep Venous Trombosis (DVT), decubiti, Myocardial Infarction (MI), renal failure, etc.">
				>
				["at0127"] = <
					text = <"Not applicable">
					description = <"*">
				>
				["at0128"] = <
					text = <"No">
					description = <"*">
				>
				["at0129"] = <
					text = <"3. Total Surgery Module Triggers">
					description = <"Total sum of Surgery Module Triggers">
				>
				["at0130"] = <
					text = <"4. Intensive Care Module Triggers">
					description = <"*">
				>
				["at0131"] = <
					text = <"40. I1 - Pneumonia onset">
					description = <"Any pneumonia diagnosed in the ICU needs to be looked at carefully. If the evidence suggests the pneumonia started prior to admission to the hospital, there is no adverse event; but if the review suggests initiation in the hospital, it is an adverse event.">
				>
				["at0132"] = <
					text = <"Not applicable">
					description = <"*">
				>
				["at0133"] = <
					text = <"No">
					description = <"*">
				>
				["at0134"] = <
					text = <"41. I2 - Readmission to intensive care">
					description = <"The unexpected admissions frequently are related to operative adverse events.">
				>
				["at0135"] = <
					text = <"Not applicable">
					description = <"*">
				>
				["at0136"] = <
					text = <"No">
					description = <"*">
				>
				["at0137"] = <
					text = <"42. I3 - In-unit procedure">
					description = <"Any procedure occurring on a patient in the intensive care unit requires investigation.">
				>
				["at0138"] = <
					text = <"Not applicable">
					description = <"*">
				>
				["at0139"] = <
					text = <"No">
					description = <"*">
				>
				["at0140"] = <
					text = <"43. I4 - Intubation or Reintubation">
					description = <"Anesthesia, sedatives, or pain medications can result in respiratory depression requiring reintubation post-operatively, which would be an adverse event.">
				>
				["at0141"] = <
					text = <"Not applicable">
					description = <"*">
				>
				["at0142"] = <
					text = <"No">
					description = <"*">
				>
				["at0143"] = <
					text = <"4. Total Intensive Care Module Triggers">
					description = <"Total sum of Intensive Care Module Triggers">
				>
				["at0144"] = <
					text = <"5. Perinatal Module Triggers">
					description = <"*">
				>
				["at0145"] = <
					text = <"44. P1 - Terbutaline use">
					description = <"Use of terbutaline could result in an unnecessary intervention of a cesarean section that is created by the administration of a medication.">
				>
				["at0146"] = <
					text = <"Not applicable">
					description = <"*">
				>
				["at0147"] = <
					text = <"No">
					description = <"*">
				>
				["at0148"] = <
					text = <"45. P2 - 3rd- or 4th-degree lacerations">
