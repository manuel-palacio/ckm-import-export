definition
	EVALUATION[at0000] matches {    -- Final diagnosis
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- Final diagnosis
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0003,    -- Primary adenocarcinoma
									at0004,    -- Squamous Cell Carcinoma
									at0005,    -- Other primary Lung malignancy
									at0006,    -- Surgery specifically for metastasis or recurrence of any malignant disease
									at0007,    -- Benign neoplastic (hamartoma, etc)
									at0008,    -- Benign non neoplastic (bullae, etc)
									at0009,    -- Other benign disease or disorder
									at0010]    -- Unknown
								}
							}
						}
					}
					ELEMENT[at0011] occurrences matches {0..1} matches {    -- Was preoperative neoadjuvant therapy given?
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0012,    -- No
									at0013,    -- Yes
									at0014]    -- Unknown
								}
							}
						}
					}
					ELEMENT[at0015] occurrences matches {0..1} matches {    -- T - Primary Tumour
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0016,    -- TX
									at0017,    -- T0
									at0018,    -- Tis
									at0019,    -- T1a(mi)
									at0020,    -- T1a
									at0021,    -- T1b
									at0022,    -- T1c
									at0023,    -- T2a
									at0024,    -- T2b
									at0025,    -- T3
									at0026,    -- T4
									at0027]    -- Unknown
								}
							}
						}
					}
					ELEMENT[at0028] occurrences matches {0..1} matches {    -- N - Regional Lymph Nodes
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0029,    -- NX
									at0030,    -- N0
									at0031,    -- N1
									at0032,    -- N2
									at0033,    -- N3
									at0034]    -- Unknown
								}
							}
						}
					}
					ELEMENT[at0035] occurrences matches {0..1} matches {    -- M - Distant Metastasis
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0036,    -- M0
									at0037,    -- M1
									at0038,    -- M1a
									at0039,    -- M1b
									at0040,    -- M1c
									at0041]    -- Unknown
								}
							}
						}
					}
					ELEMENT[at0042] occurrences matches {0..1} matches {    -- Notes on final diagnosis
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0043] occurrences matches {0..1} matches {    -- Notes
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

