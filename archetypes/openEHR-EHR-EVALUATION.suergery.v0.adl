definition
	EVALUATION[at0000] matches {    -- Suergery
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0003] occurrences matches {0..1} matches {    -- Operation actually performed
						value matches {
							0|[local::at0004],
							1|[local::at0005],
							2|[local::at0006]
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {    -- Date of primary operation
						value matches {
							DV_DATE matches {*}
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {    -- Surgery scheduling type
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0009,    -- Elective surgery
									at0010,    -- Operation after unplanned admission (Planned surgery after unplanned admission)
									at0011,    -- Acute surgery (2-24 hrs) (The patient's condition necessitated operation within 2 to 24 hours)
									at0012]    -- Emergency surgery (<2 hrs) (The patient's condition necessitated operation within 2 hours)
								}
							}
						}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {    -- Primary operator
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0014] occurrences matches {0..1} matches {    -- Primary procedure name
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0015,    -- Wedge resection
									at0016,    -- Bi-Lobectomy
									at0017,    -- Segmentectomy
									at0018,    -- Pneumonectomy
									at0019,    -- Lobectomy
									at0020]    -- Other
								}
							}
						}
					}
					ELEMENT[at0021] occurrences matches {0..1} matches {    -- Location of Primary procedure resection
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0022,    -- RUL
									at0023,    -- RML
									at0024,    -- LUL
									at0025,    -- RLL
									at0026]    -- LLL
								}
							}
						}
					}
					ELEMENT[at0027] occurrences matches {0..1} matches {    -- Secondary procedure name
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0028,    -- None
									at0029,    -- Lobectomy
									at0030,    -- Wedge resection
									at0031,    -- Bi-Lobectomy
									at0032,    -- Segmentectomy
									at0033,    -- Pneumonectomy
									at0034,    -- Other
									at0035]    -- Unknown
								}
							}
						}
					}
					ELEMENT[at0036] occurrences matches {0..1} matches {    -- Location of Secondary Procedure resection
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0037,    -- RUL
									at0038,    -- RML
									at0039,    -- RLL
									at0040,    -- LUL
									at0041]    -- LLL
								}
							}
						}
					}
					ELEMENT[at0042] occurrences matches {0..1} matches {    -- Lymphadenectomy
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0043,    -- No dissection
									at0044,    -- Sampling
									at0045]    -- Mediastinal lymph node dissection
								}
							}
						}
					}
					ELEMENT[at0046] occurrences matches {0..1} matches {    -- Surgical approach
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0047,    -- Open surgery - Thoracotomy
									at0048,    -- Standard VATS (2-4 ports)
									at0049,    -- Uniportal VATS
									at0050,    -- Converted from VATS
									at0051,    -- Robotic
									at0052,    -- Converted from robotic
									at0053]    -- Unknown
								}
							}
						}
					}
					ELEMENT[at0054] occurrences matches {0..1} matches {    -- Muscle-sparing Thoracotomy Incision
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0055,    -- No
									at0056,    -- Yes
									at0057]    -- Unknown
								}
							}
						}
					}
					ELEMENT[at0058] occurrences matches {0..1} matches {    -- ICM Flap
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0059,    -- No
									at0060,    -- Yes
									at0061]    -- Unknown
								}
							}
						}
					}
					ELEMENT[at0062] occurrences matches {0..1} matches {    -- Rib re-approximation avoiding nerve compression
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0063,    -- No
									at0064,    -- Yes
									at0065]    -- Unknown
								}
							}
						}
					}
					ELEMENT[at0066] occurrences matches {0..1} matches {    -- Number of Chest tubes
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0067,    -- None
									at0068,    -- 1
									at0069,    -- 2 or more
									at0070]    -- Unknown
								}
							}
						}
					}
					ELEMENT[at0071] occurrences matches {0..1} matches {    -- Intraoperative blood loss
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::129]>
								list = <
									["1"] = <
										units = <"mL">
									>
								>
							>
						}
					}
					ELEMENT[at0072] occurrences matches {0..1} matches {    -- Was Chlorhexidine used during skin prep?
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0073,    -- No
									at0074,    -- Yes
									at0075]    -- Unknown
								}
							}
						}
					}
					ELEMENT[at0077] occurrences matches {0..1} matches {    -- Start of operation (time)
						value matches {
							DV_TIME matches {*}
						}
					}
					ELEMENT[at0078] occurrences matches {0..1} matches {    -- Stop of operation (time)
						value matches {
							DV_TIME matches {*}
						}
					}
					ELEMENT[at0079] occurrences matches {0..1} matches {    -- Stop of operation (date)
						value matches {
							DV_TIME matches {*}
						}
					}
					ELEMENT[at0080] occurrences matches {0..1} matches {    -- Urinary drainage postop
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0081,    -- No drainage
									at0082,    -- Urethral/Foley Catheter
									at0083,    -- Nephrostomy
									at0084,    -- Suprapubic catheter
									at0085]    -- Unknown
								}
							}
						}
					}
					ELEMENT[at0086] occurrences matches {0..1} matches {    -- Notes
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

