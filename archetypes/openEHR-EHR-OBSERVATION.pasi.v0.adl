									CLUSTER[at0004] occurrences matches {0..1} matches {	-- Head and neck
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0005] occurrences matches {0..1} matches {	-- Erythema
												value matches {
													0|[local::at0006], 	-- None
													1|[local::at0007], 	-- Slight
													2|[local::at0008], 	-- Moderate
													3|[local::at0009], 	-- Severe
													4|[local::at0010]  	-- Very severe
												}
											}
											ELEMENT[at0011] occurrences matches {0..1} matches {	-- Induration
												value matches {
													0|[local::at0006], 	-- None
													1|[local::at0007], 	-- Slight
