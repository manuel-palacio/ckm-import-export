                <term_definitions code="at0059">
                    <items id="text">V.A.T.</items>
                    <items id="description">Ventricular Activation Time is the interval from the onset of the QRS complex to the latest positive peak in the complex, or the latest substantial notch on the latest peak (whichever is later).</items>
                </term_definitions>
                <term_definitions code="at0060">
                    <items id="text">QRS p-p</items>
                    <items id="description">Peak-to-peak QRS complex amplitude.</items>
                </term_definitions>
                <term_definitions code="at0061">
                    <items id="text">QRS duration</items>
                    <items id="description">QRS complex duration, measured from its onset to the ST segment onset (J point).</items>
                </term_definitions>
                <term_definitions code="at0062">
                    <items id="text">QRS area</items>
                    <items id="description">The area of the QRS complex.</items>
                </term_definitions>
                <term_definitions code="at0063">
                    <items id="text">ST onset</items>
                    <items id="description">Elevation or depression at the onset (J point) of the ST segment.</items>
                </term_definitions>
                <term_definitions code="at0064">
                    <items id="text">ST midpoint</items>
                    <items id="description">Elevation or depression at the midpoint of the ST segment.</items>
                </term_definitions>
                <term_definitions code="at0065">
                    <items id="text">ST 80ms</items>
                    <items id="description">Elevation or depression of the ST segment 80 ms after the end of the QRS complex (J point).</items>
                </term_definitions>
                <term_definitions code="at0066">
                    <items id="text">ST end</items>
                    <items id="description">Elevation or depression at the end of the ST segment.</items>
                </term_definitions>
                <term_definitions code="at0067">
                    <items id="text">ST duration</items>
                    <items id="description">ST segment duration.</items>
                </term_definitions>
                <term_definitions code="at0068">
                    <items id="text">ST slope</items>
                    <items id="description">ST segment slope. Slope is measured in degrees for 25 mm/sec, 1mV/cm scaling.</items>
                </term_definitions>
                <term_definitions code="at0069">
                    <items id="text">ST shape</items>
                    <items id="description">The ST segment shape.</items>
                </term_definitions>
                <term_definitions code="at0070">
                    <items id="text">Straight</items>
                    <items id="description">The ST segment is straight.</items>
                </term_definitions>
                <term_definitions code="at0071">
                    <items id="text">Concave upward</items>
                    <items id="description">The ST segment is concave upward.</items>
                </term_definitions>
                <term_definitions code="at0072">
                    <items id="text">Concave downward</items>
                    <items id="description">The ST segment is concave downward.</items>
                </term_definitions>
