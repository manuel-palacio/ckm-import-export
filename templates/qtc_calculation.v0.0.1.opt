    <component_ontologies archetype_id="openEHR-EHR-OBSERVATION.pulse.v2">
        <term_definitions language="ar-sy">
            <items code="at0001">
                <items id="text">*structure(en)</items>
                <items id="description">*@ internal @(en)</items>
            </items>
            <items code="at0002">
                <items id="text">*history(en)</items>
                <items id="description">*@ internal @(en)</items>
            </items>
            <items code="at0003">
                <items id="text">*Any event(en)</items>
                <items id="description">*Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.(en)</items>
            </items>
            <items code="at0004">
                <items id="text">*Rate(en)</items>
                <items id="description">*The rate of the pulse or heart beat, measured in beats per minute. (en)</items>
            </items>
            <items code="at0005">
                <items id="text">*Regular?(en)</items>
                <items id="description">*Is the pulse or heart beat regular?(en)</items>
            </items>
            <items code="at0006">
                <items id="text">*Regular(en)</items>
                <items id="description">*The pattern is regular.(en)</items>
            </items>
            <items code="at0007">
                <items id="text">*Regularly Irregular(en)</items>
                <items id="description">*The pattern is irregular in a regular pattern,. For example, a dropped beat once every 'n' beats.(en)</items>
            </items>
            <items code="at0008">
                <items id="text">*Irregularly Irregular(en)</items>
                <items id="description">*The pattern is irregular in a chaotic and unpredictable manner. For example, atrial fibrillation.(en)</items>
            </items>
            <items code="at0010">
                <items id="text">*List(en)</items>
                <items id="description">*@ internal @(en)</items>
            </items>
            <items code="at0012">
                <items id="text">*List(en)</items>
                <items id="description">*@ internal @(en)</items>
            </items>
            <items code="at0013">
                <items id="text">*Position(en)</items>
                <items id="description">*The body position of the subject during the observation.(en)</items>
            </items>
            <items code="at1000">
                <items id="text">*Lying(en)</items>
                <items id="description">*The subject was lying flat.(en)</items>
            </items>
            <items code="at1001">
                <items id="text">*Sitting(en)</items>
                <items id="description">*The subject was sitting (for example on bed or chair).(en)</items>
            </items>
            <items code="at1002">
                <items id="text">*Reclining(en)</items>
                <items id="description">*The subject was reclining at an approximate angle of 45 degrees, with the legs elevated to the level of the pelvis.(en)</items>
            </items>
            <items code="at1003">
                <items id="text">*Standing/upright(en)</items>
                <items id="description">*The subject was standing, walking or running.(en)</items>
            </items>
            <items code="at1005">
                <items id="text">*Presence(en)</items>
                <items id="description">*Presence of a pulse or heart beat.(en)</items>
                <items id="comment">*It can be implied that the pulse or heart beat is present if Rate &gt;0 /min. (en)</items>
            </items>
            <items code="at1013">
                <items id="text">*Device(en)</items>
                <items id="description">*Details about the device used to measure the pulse rate or heart rate.(en)</items>
            </items>
            <items code="at1017">
                <items id="text">*Exertion(en)</items>
                <items id="description">*Details about physical exertion being undertaken during the examination.(en)</items>
            </items>
            <items code="at1018">
                <items id="text">*Confounding factors(en)</items>
                <items id="description">*Narrative description about any incidental factors that may affect interpretation of the physical findings.(en)</items>
                <items id="comment">*For example, presence of a pacemaker, level of anxiety; pain or fever etc.(en)</items>
            </items>
            <items code="at1019">
                <items id="text">*Method(en)</items>
                <items id="description">*Method used to observe the pulse or heart beat.(en)</items>
                <items id="comment">*For example, auscultation or electronic monitoring. (en)</items>
            </items>
            <items code="at1022">
                <items id="text">*Clinical description(en)</items>
                <items id="description">*Narrative description about the pulse or heart beat.(en)</items>
            </items>
            <items code="at1023">
                <items id="text">*Clinical interpretation(en)</items>
                <items id="description">*Single word, phrase or brief description that represents the clinical meaning and significance of the pulse or heart beat findings, including the rhythm.(en)</items>
                <items id="comment">*Coding with a terminology is preferred, where possible. For example: Bradycardia, Extrasystoles or Sinus rhythm. Multiple statements are allowed. (en)</items>
            </items>
            <items code="at1024">
                <items id="text">*Present(en)</items>
                <items id="description">*A pulse or heart beat can be detected.(en)</items>
            </items>
            <items code="at1025">
                <items id="text">*Not detected(en)</items>
                <items id="description">*A pulse or heart beat cannot be detected.(en)</items>
            </items>
            <items code="at1028">
                <items id="text">*Irregular(en)</items>
                <items id="description">*The pattern is irregular.(en)</items>
            </items>
            <items code="at1030">
                <items id="text">*Character(en)</items>
                <items id="description">*Description of the character of the pulse or heart beat.(en)</items>
                <items id="comment">*Coding with a terminology is desired, where possible. For example: full, thready, bounding, slow rising, or collapsing. Multiple terms may be recorded.(en)</items>
            </items>
            <items code="at1032">
                <items id="text">*Palpation(en)</items>
                <items id="description">*The findings are observed by physical touch of the observer on the subject.(en)</items>
            </items>
            <items code="at1033">
                <items id="text">*Auscultation(en)</items>
                <items id="description">*The findings are observed with the assistance of a device, such as a stethoscope.(en)</items>
            </items>
            <items code="at1034">
                <items id="text">*Automatic, non-invasive(en)</items>
                <items id="description">*The findings are observed non-invasively using a device such as a pulse oximeter or a stethoscope.(en)</items>
            </items>
            <items code="at1036">
                <items id="text">*Maximum(en)</items>
                <items id="description">*Maximum pulse rate or heart rate observed during a period of exertion.(en)</items>
            </items>
            <items code="at1037">
                <items id="text">*Body site(en)</items>
                <items id="description">*Body site where the pulse or heart beat were observed.(en)</items>
            </items>
            <items code="at1038">
                <items id="text">*Radial Artery - Left(en)</items>
                <items id="description">*The left radial artery.(en)</items>
            </items>
            <items code="at1039">
                <items id="text">*Radial Artery - Right(en)</items>
                <items id="description">*The right radial artery.(en)</items>
            </items>
            <items code="at1040">
                <items id="text">*Heart(en)</items>
                <items id="description">*The region of the heart.(en)</items>
            </items>
            <items code="at1041">
                <items id="text">*Carotid Artery - Left(en)</items>
                <items id="description">*The left carotid artery.(en)</items>
            </items>
            <items code="at1042">
                <items id="text">*Carotid Artery - Right(en)</items>
                <items id="description">*The right carotid artery.(en)</items>
            </items>
            <items code="at1043">
                <items id="text">*Femoral Artery - Left(en)</items>
                <items id="description">*The left femoral artery.(en)</items>
            </items>
            <items code="at1044">
                <items id="text">*Femoral Artery - Right(en)</items>
                <items id="description">*The right femoral artery.(en)</items>
            </items>
            <items code="at1047">
                <items id="text">*Finger(en)</items>
                <items id="description">*An unspecified finger.(en)</items>
            </items>
            <items code="at1048">
                <items id="text">*Brachial artery - Left(en)</items>
                <items id="description">*The left brachial artery.(en)</items>
            </items>
            <items code="at1049">
                <items id="text">*Brachial artery - Right(en)</items>
                <items id="description">*The right brachial artery.(en)</items>
            </items>
            <items code="at1050">
                <items id="text">*Automatic, invasive(en)</items>
                <items id="description">*The findings are observed invasively using a device such as an arterial catheter.(en)</items>
            </items>
            <items code="at1051">
                <items id="text">*Ear lobe(en)</items>
                <items id="description">*The lobe of an unspecified ear.(en)</items>
            </items>
            <items code="at1054">
                <items id="text">*Toe(en)</items>
                <items id="description">*An unspecified toe.(en)</items>
            </items>
            <items code="at1055">
                <items id="text">*Irregular type(en)</items>
                <items id="description">*More specific pattern of an irregular pulse or heart beat.(en)</items>
                <items id="comment">*Selection of a value from this value set is only valid if 'Irregular' is selected from the 'Regularity' data element.(en)</items>
            </items>
            <items code="at1056">
                <items id="text">*Extension(en)</items>
                <items id="description">*Additional information required to capture local content or to align with other reference models/formalisms.(en)</items>
                <items id="comment">*For example: local information requirements or additional metadata to align with FHIR or CIMI equivalents.(en)</items>
            </items>
            <items code="at1057">
                <items id="text">*Clinical interpretation(en)</items>
                <items id="description">*Generic label to allow for any or all statements about the pulse or heart beat.(en)</items>
            </items>
            <items code="at1058">
                <items id="text">*Rhythm(en)</items>
                <items id="description">*Specific conclusion about the rhythm of the pulse or heartbeat, drawn from a combination of the heart rate, pattern and other characteristics observed on examination.(en)</items>
            </items>
            <items code="at1059">
                <items id="text">*Comment(en)</items>
                <items id="description">*Additional narrative about the pulse or heart beat findings not captured in other fields.(en)</items>
            </items>
            <items code="at0000">
                <items id="text">*Pulse/Heart beat(en)</items>
                <items id="description">*The rate and associated attributes for a pulse or heart beat. (en)</items>
            </items>
        </term_definitions>
