        <term_definitions language="es-ar">
            <items code="at0000">
                <items id="text">GCS</items>
                <items id="description">Interaccin, contacto o evento de cuidados entre un sujeto de cuidados y uno o ms proveedores de cuidados de la salud.</items>
            </items>
            <items code="at0001">
                <items id="text">*Tree(en)</items>
                <items id="description">*@ internal @(en)</items>
            </items>
            <items code="at0002">
