                    <items id="text">Harris Benedict BMR</items>
                    <items id="description">Estimated Basal Metabolic Rate (BMR) based on the Harris-Benedict equations.</items>
                </term_definitions>
                <term_definitions code="at0005">
                    <items id="text">Schofield BMR</items>
                    <items id="description">Estimated Basal Metabolic Rate (BMR) based on the Schofield equations.</items>
                </term_definitions>
                <term_definitions code="at0006">
                    <items id="text">Revised Harris Benedict BMR</items>
                    <items id="description">Estimated Basal Metabolic Rate (BMR) based on the revised Harris-Benedict equations.</items>
                </term_definitions>
                <term_definitions code="at0007">
                    <items id="text">Mifflin St Jeor BMR</items>
                    <items id="description">Predicted Resting Metabolic Rate (RMR) based on the Mifflin St Jeor equations.</items>
                </term_definitions>
                <term_definitions code="at0008">
                    <items id="text">Katch McArdle Cunningham BMR</items>
                    <items id="description">Predicted Resting Daily Energy Expenditure (RDEE) based on the Katch-McArdle and Cunningham equations.</items>
                </term_definitions>
