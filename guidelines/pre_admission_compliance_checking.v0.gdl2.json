    "rules": {
      "gt0013": {
        "id": "gt0013",
        "priority": 7,
        "when": [
          "($gt0011|Preoperative nutritional status assessment|==1|local::at0008|Yes, normal status|)||(($gt0011|Preoperative nutritional status assessment|==2|local::at0009|Yes, risk of malnutrition|)||($gt0011|Preoperative nutritional status assessment|==3|local::at0010|Yes, malnourished|))",
          "$gt0012|Screening instrument|!=0|local::at0013|None|",
          "$gt0012|Screening instrument|!=9|local::at0021|Unknown|"
        ],
        "then": [
          "$gt0004|Preoperative nutritional status assessment|=local::at0003|Compliant|"
        ]
      },
      "gt0014": {
        "id": "gt0014",
        "priority": 6,
        "when": [
          "($gt0011|Preoperative nutritional status assessment|==1|local::at0008|Yes, normal status|)||(($gt0011|Preoperative nutritional status assessment|==2|local::at0009|Yes, risk of malnutrition|)||($gt0011|Preoperative nutritional status assessment|==3|local::at0010|Yes, malnourished|))",
          "($gt0012|Screening instrument|==0|local::at0013|None|)||($gt0012|Screening instrument|==9|local::at0021|Unknown|)"
        ],
        "then": [
          "$gt0004|Preoperative nutritional status assessment|=local::at0004|Non-compliant|"
        ]
      },
      "gt0015": {
        "id": "gt0015",
        "priority": 5,
        "when": [
          "$gt0011|Preoperative nutritional status assessment|==0|local::at0007|No, not assessed|"
        ],
        "then": [
          "$gt0004|Preoperative nutritional status assessment|=local::at0004|Non-compliant|"
        ]
      },
      "gt0016": {
        "id": "gt0016",
        "priority": 4,
        "when": [
          "($gt0011|Preoperative nutritional status assessment|==9|local::at0011|Unknown|)||($gt0012|Screening instrument|==9|local::at0021|Unknown|)"
        ],
        "then": [
          "$gt0004|Preoperative nutritional status assessment|=local::at0005|Missing|"
        ]
      },
      "gt0017": {
        "id": "gt0017",
        "priority": 3,
        "when": [
          "$gt0018|Smoker|==local::at0108|Stopped because of surgery|",
          "$gt0019|Termination of smoking (no. weeks before surgery)|>=4"
        ],
        "then": [
          "$gt0009|Smoker|=local::at0003|Compliant|"
        ]
      },
      "gt0020": {
        "id": "gt0020",
        "priority": 2,
        "when": [
          "$gt0018|Smoker|==local::at0108|Stopped because of surgery|",
          "$gt0019|Termination of smoking (no. weeks before surgery)|<4"
        ],
        "then": [
          "$gt0009|Smoker|=local::at0004|Non-compliant|"
        ]
      },
      "gt0021": {
        "id": "gt0021",
        "priority": 1,
        "when": [
          "$gt0018|Smoker|==local::at0108|Stopped because of surgery|",
          "$gt0019|Termination of smoking (no. weeks before surgery)|==null"
        ],
        "then": [
          "$gt0009|Smoker|=local::at0005|Missing|"
        ]
      }
    }
  },
