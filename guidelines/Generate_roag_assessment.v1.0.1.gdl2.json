          "$gt0401|Count of 3s|=($gt0019.term + $gt0008.string)"
        ]
      },
      "gt0102": {
        "id": "gt0102",
        "priority": 13,
        "when": [
          "$gt0007|Count of risk points of 2|!=null"
        ],
        "then": [
          "$gt0501|Count of 2s|=($gt0018.term+$gt0007.string)"
        ]
      },
      "gt0104": {
        "id": "gt0104",
        "priority": 12,
        "when": [
          "$gt0008|Count of risk points of 3|.magnitude>=1"
        ],
        "then": [
          "$gt0080|Risk level|=$gt0090.term",
          "$gt0081|Recommendation|=$gt0091.term",
          "$gt0602|severity|='high'",
          "$gt0601|riskLevel|=$gt0084.term",
          "$gt0701|recommendation|=$gt0087.term",
          "$gt0807|activateCareplanInitialValue|=local::at0007|Ja|"
        ]
      },
      "gt0105": {
        "id": "gt0105",
        "priority": 11,
        "when": [
          "$gt0008|Count of risk points of 3|.magnitude<1",
          "$gt0007|Count of risk points of 2|.magnitude>=1"
        ],
        "then": [
          "$gt0080|Risk level|=$gt0092.term",
          "$gt0081|Recommendation|=$gt0093.term",
          "$gt0602|severity|='moderate'",
          "$gt0601|riskLevel|=$gt0085.term",
          "$gt0701|recommendation|=$gt0088.term",
          "$gt0807|activateCareplanInitialValue|=local::at0007|Ja|"
        ]
      },
      "gt0106": {
        "id": "gt0106",
        "priority": 10,
        "when": [
          "$gt0008|Count of risk points of 3|.magnitude<1",
          "$gt0007|Count of risk points of 2|.magnitude<1"
        ],
        "then": [
          "$gt0080|Risk level|=$gt0094.term",
          "$gt0081|Recommendation|=$gt0095.term",
          "$gt0602|severity|='none'",
          "$gt0601|riskLevel|=$gt0086.term",
          "$gt0701|recommendation|=$gt0089.term",
          "$gt0807|activateCareplanInitialValue|=local::at0008|Nej|"
        ]
      },
      "gt0112": {
