    "rules": {
      "gt0015": {
        "id": "gt0015",
        "priority": 4,
        "when": [
          "$gt0007|N - Regional Lymph Nodes|==local::at0030|N0|",
          "$gt0008|M - Distant Metastasis|==local::at0036|M0|",
          "($gt0006|T - Primary Tumour|==local::at0020|T1a|)||(($gt0006|T - Primary Tumour|==local::at0021|T1b|)||(($gt0006|T - Primary Tumour|==local::at0019|T1a(mi)|)||(($gt0006|T - Primary Tumour|==local::at0022|T1c|)||(($gt0006|T - Primary Tumour|==local::at0023|T2a|)||($gt0006|T - Primary Tumour|==local::at0024|T2b|)))))"
        ],
        "then": [
          "$gt0014|early-stage lung cancer|=true"
        ]
      },
      "gt0017": {
        "id": "gt0017",
        "priority": 3,
        "when": [
          "$gt0014|early-stage lung cancer|==true",
          "($gt0010|Surgical approach|==local::at0048|Standard VATS (2-4 ports)|)||($gt0010|Surgical approach|==local::at0049|Uniportal VATS|)"
        ],
        "then": [
          "$gt0004|Surgical Technique - MIS|=local::at0004|Compliant|"
        ]
      },
      "gt0018": {
        "id": "gt0018",
        "priority": 2,
        "when": [
          "$gt0014|early-stage lung cancer|==true",
          "($gt0010|Surgical approach|==local::at0047|Open surgery - Thoracotomy|)||(($gt0010|Surgical approach|==local::at0052|Converted from robotic|)||($gt0010|Surgical approach|==local::at0050|Converted from VATS|))"
        ],
        "then": [
          "$gt0004|Surgical Technique - MIS|=local::at0005|Non Compliant|"
        ]
      },
      "gt0019": {
        "id": "gt0019",
        "priority": 1,
        "when": [
          "($gt0010|Surgical approach|==local::at0051|Robotic|)||(($gt0014|early-stage lung cancer|!=true)||(($gt0007|N - Regional Lymph Nodes|!=local::at0030|N0|)||($gt0008|M - Distant Metastasis|!=local::at0036|M0|)))",
          "$gt0006|T - Primary Tumour|!=local::at0027|Unknown|",
          "$gt0007|N - Regional Lymph Nodes|!=local::at0034|Unknown|",
          "$gt0008|M - Distant Metastasis|!=local::at0041|Unknown|"
        ],
        "then": [
          "$gt0004|Surgical Technique - MIS|=local::at0007|Not Applicable|"
        ]
      }
    }
  },
