    "rules": {
      "gt0031": {
        "id": "gt0031",
        "priority": 6,
        "when": [
          "$gt0005|Clinical signs and symptoms of DVT|!=null",
          "$gt0006|Is PE the number one diagnosis OR equally likely|!=null",
          "$gt0008|Immobilization at least 3 days OR surgery in the Previous 4 weeks|!=null",
          "$gt0009|Previous objectively diagnosed PE or DVT|!=null",
          "$gt0010|Hemoptysis|!=null",
          "$gt0011|Malignancy with treatment within 6 months or palliative|!=null"
        ],
        "then": [
          "$gt0021|Total score|.magnitude=($gt0005.value+$gt0006.value+$gt0008.value+$gt0009.value+$gt0010.value+$gt0011.value+$gt0056.value)/2",
          "$gt0021|Total score|.unit='1'",
          "$gt0021|Total score|.precision=1"
        ]
      },
      "gt0038": {
        "id": "gt0038",
        "priority": 5,
        "when": [
          "$gt0021|Total score|.magnitude<=1.5",
		  "$gt0021|Total score|.magnitude>=0"
        ],
        "then": [
          "$gt0036|Risk Incidence|='12.1%'",
          "$gt0035|Risk Prevelance|='1.3%'",
          "$gt0045|Interpretation text 2 tier risk|=$gt0051.term",
          "$gt0044|Interpretation text 3 tier risk|=$gt0053.term",
          "$gt0046|Interpretation text recommendation|=$gt0047.term"
        ]
      },
      "gt0039": {
        "id": "gt0039",
        "priority": 4,
        "when": [
