          "$gt0021|Total score|.magnitude<=4.5"
        ],
        "then": [
          "$gt0036|Risk Incidence|='12.1%'",
          "$gt0035|Risk Prevelance|='16.2%'",
          "$gt0045|Interpretation text 2 tier risk|=$gt0051.term",
          "$gt0044|Interpretation text 3 tier risk|=$gt0054.term",
          "$gt0046|Interpretation text recommendation|=$gt0048.term"
        ]
      },
      "gt0042": {
        "id": "gt0042",
        "priority": 3,
        "when": [
          "$gt0021|Total score|.magnitude<=6",
          "$gt0021|Total score|.magnitude>=5"
        ],
