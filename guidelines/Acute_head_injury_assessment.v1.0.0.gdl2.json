    "rules": {
      "gt0015": {
        "id": "gt0015",
        "priority": 8,
        "when": [
          "$gt0018|age>=18,a|==true",
          "$gt0010|GCS Total score|<=13",
          "$gt0010|GCS Total score|>=9"
        ],
        "then": [
          "$gt0012|Head injury level|=local::at0020|Medelsvår|",
          "$gt0014|Recommendation|=local::at0027|Datortomografi (DT) undersökning|"
        ]
      },
      "gt0016": {
        "id": "gt0016",
        "priority": 7,
        "when": [
          "$gt0018|age>=18,a|==true",
          "$gt0010|GCS Total score|<=15",
          "$gt0010|GCS Total score|>=14",
          "$gt0007|Clinical signs of depressed or basal skull fracture|==true||$gt0008|Focal neurological deficits|==true||$gt0005|Anticoagulation therapy or coagulation disorders|==true||$gt0006|Posttraumatic seizures|==true||$gt0017|Shunt-treated hydrocephalus|==true"
        ],
        "then": [
          "$gt0012|Head injury level|=local::at0019|Lätt|",
          "$gt0013|Risk level|=local::at0024|Hög|",
          "$gt0014|Recommendation|=local::at0027|Datortomografi (DT) undersökning|"
        ]
      },
      "gt0020": {
        "id": "gt0020",
        "priority": 6,
        "when": [
          "$gt0018|age>=18,a|==true",
          "$gt0012|Head injury level|==null",
          "$gt0010|GCS Total score|<=15",
          "$gt0010|GCS Total score|>=14",
          "$gt0066|patient-age|!=null",
          "$gt0066|patient-age|.magnitude>=65",
          "$gt0021|Antiplatelet medication|==true"
        ],
        "then": [
          "$gt0012|Head injury level|=local::at0019|Lätt|",
          "$gt0013|Risk level|=local::at0023|Måttlig|",
          "$gt0014|Recommendation|=local::at0027|Datortomografi (DT) undersökning|"
        ]
      },
      "gt0085": {
        "id": "gt0085",
        "priority": 5,
        "when": [
          "$gt0018|age>=18,a|==true",
          "$gt0012|Head injury level|==null",
          "$gt0010|GCS Total score|==14",
          "$gt0025|Estimated time of injury|.code=='at0035'||$gt0025|Estimated time of injury|.code=='at0036'"
        ],
        "then": [
          "$gt0012|Head injury level|=local::at0019|Lätt|",
          "$gt0013|Risk level|=local::at0022|Låg|",
          "$gt0014|Recommendation|=local::at0027|Datortomografi (DT) undersökning|"
        ]
      },
      "gt0086": {
        "id": "gt0086",
        "priority": 4,
        "when": [
          "$gt0018|age>=18,a|==true",
          "$gt0012|Head injury level|==null",
          "$gt0010|GCS Total score|==15",
          "$gt0025|Estimated time of injury|.code=='at0035'||$gt0025|Estimated time of injury|.code=='at0036'",
          "$gt0003|Suspected loss of consciousness|==true||$gt0004|Repeated (≥2) vomiting|==true"
        ],
        "then": [
          "$gt0012|Head injury level|=local::at0019|Lätt|",
          "$gt0013|Risk level|=local::at0022|Låg|",
          "$gt0014|Recommendation|=local::at0027|Datortomografi (DT) undersökning|"
        ]
      },
      "gt0022": {
        "id": "gt0022",
        "priority": 3,
        "when": [
          "$gt0018|age>=18,a|==true",
          "$gt0012|Head injury level|==null",
          "$gt0010|GCS Total score|==14"
        ],
        "then": [
          "$gt0012|Head injury level|=local::at0019|Lätt|",
          "$gt0013|Risk level|=local::at0022|Låg|",
          "$gt0014|Recommendation|=local::at0028|S100B analys|"
